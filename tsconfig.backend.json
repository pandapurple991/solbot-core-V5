{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowImportingTsExtensions": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "outDir": "./dist-backend",
    "rootDir": ".",
    "strict": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": false,
    "sourceMap": true
  },
  "include": [
    "backend/src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "src",
    "dist",
    "dist-backend",
    "backend/src/startTradingLiquidity.ts",
    "backend/src/startTradingLiquidity2.ts"
  ]
}